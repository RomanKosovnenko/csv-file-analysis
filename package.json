{
  "name": "csv-file-analysis",
  "version": "1.0.0",
  "description": "Es soll eine Anwendung erstellt werden, die Daten aus einer CSV-Datei analysiert und HTTP-Endpunkte bereitstellt, um Statistiken und Visualisierungen abzufragen. Die Anwendung soll in einem Docker-Container laufen.",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "nodemon",
    "test": "jest"
  },
  "keywords": [],
  "author": "Roman Kosovnenko",
  "license": "ISC",
  "dependencies": {
    "csv-parser": "^3.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "data"
    ],
    "ext": "ts",
    "exec": "ts-node src/app.ts"
  }
}
